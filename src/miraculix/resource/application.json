{
  "application": {
    "name": "miraculix",
    "version": "0.0",
    "dbHost": "thot-container",
    "devMode": false
  },
  "dobby": {
    "staticContent": {
      "directory": "static"
    },
    "session": {
      "age": 2,
      "store": "hades.session.HadesSessionStore"
    },
    "port": 1711,
    "threads": 10,
    "mode": "http",
    "logLevel": "DEBUG"
  },
  "hades": {
    "disableWelcomeMessage": true,
    "apidocs": {
      "enabled": true,
      "hideHadesRoutes": true
    },
    "login": {
      "maxAttempts": 5,
      "lockDuration": 300000,
      "tokenHeaderName": "Hades-Login-Token",
      "redirect": {
        "success": "/",
        "successAdmin": "/hades"
      }
    },
    "unauthorizedRedirectTarget": "/hades/login",
    "unauthorizedRedirectPaths": [
      "/",
      "/index.html"
    ]
  }
}